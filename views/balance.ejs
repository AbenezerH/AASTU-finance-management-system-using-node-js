                    <th scope="col">Outgoing Amount</th>
                    <th scope="col">Balance</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
        
                <% print.forEach(function (user) { %>
                    <tr>
                        <td><%= user.ID %></td>
                        <td><%= user.Amount %></td>
                       
                        <% }) %>
        
                </tr>
        
            </tbody>
        </table>
